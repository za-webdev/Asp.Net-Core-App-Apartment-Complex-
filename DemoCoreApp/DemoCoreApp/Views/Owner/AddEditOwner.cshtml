@model DemoCoreApp.Models.Owner

<form asp-controller="Home" asp-action="AddUpdateAccount" method="post">
    <div>
        <label asp-for="FirstName">First Name</label>
        <input asp-for="FirstName" />
    </div>
    <div>
        <label asp-for="LastName">Last Name</label>
        <input asp-for="LastName" />
    </div>
    <div>
        <label asp-for="Email">Email</label>
        <input asp-for="Email" />
    </div>
    <div>
        <label asp-for="PhoneNumber">Phone #</label>
        <input asp-for="PhoneNumber" />
    </div>
    <div>
        <label asp-for="ApartmentNumber">Apartment Number</label>
        <input asp-for="ApartmentNumber" />
    </div>
    <div>
        <label asp-for="UnitNumber">Unit Number</label>
        <input asp-for="UnitNumber" />
    </div>
    <div>
        <button type="submit">Create</button>
    </div>

    
</form>
