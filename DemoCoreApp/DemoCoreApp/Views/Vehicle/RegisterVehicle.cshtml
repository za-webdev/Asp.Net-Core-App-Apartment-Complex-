@model DemoCoreApp.ViewModels.VehicleViewModel
@{
    ViewData["Title"] = "Register Vehicle";
    ViewData["ShowRegNum"] = true;
}


<h2>Register</h2>
<div class="row">
    <div class="col-md-12">
        <form method="post" action="RegisterVehicle">
           <partial name="_VehicleFormPartialView" model="Model" view-data="ViewData" />
            <div class="form-group row">
                <div class="col-sm-10">
                    <button type="submit" class="btn btn-primary">Register</button>
                    <a asp-controller="Home" asp-action="" class="btn btn-dangerous">Cancle</a>
                </div>

            </div>
           
        </form>
    </div>

</div>

